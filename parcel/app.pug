    script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyCu-BASkqxhAYMOVu25daGmDr5_na6IAW4&callback=initMap&v=weekly', defer='')

    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/html5-qrcode/2.2.1/html5-qrcode.min.js')
